{
    "problemMatcher": [
        {
            "owner": "svelte-check",
            "pattern": [
                {
                    "regexp": "^([^:]+):([0-9]+):([0-9]+)\\s*-\\s*(error|warning)\\s*-\\s*(.+)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                },
                {
                    "regexp": "^([^:]+):([0-9]+):([0-9]+):\\s*(.+)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4
                }
            ]
        }
    ]
}
