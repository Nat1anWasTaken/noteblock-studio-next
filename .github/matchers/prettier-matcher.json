{
    "problemMatcher": [
        {
            "owner": "prettier",
            "pattern": [
                {
                    "regexp": "^\\[error\\]\\s*([^:]+):\\s*(.+)\\s*\\(([0-9]+):([0-9]+)\\)$",
                    "file": 1,
                    "message": 2,
                    "line": 3,
                    "column": 4
                },
                {
                    "regexp": "^\\[warn\\]\\s*(.+)$",
                    "file": 1,
                    "message": "Code style issues found"
                }
            ]
        }
    ]
}
